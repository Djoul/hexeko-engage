APP_NAME=UpPlus+
APP_ENV=production
API_VERSION=0.1.0-dev
API_VERSION_PATH=v1
APP_KEY=base64:xxxxxxx
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost:1310

APP_LOCALE=en-GB
APP_FALLBACK_LOCALE=en-GB
APP_FAKER_LOCALE=en_GB

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=daily
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

DB_CONNECTION=pgsql
DB_HOST=db_engage
DB_PORT=5432
DB_DATABASE=db_engage
DB_USERNAME=root
DB_PASSWORD=password

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=reverb
FILESYSTEM_DISK=s3-local
MEDIA_DISK=s3-local
QUEUE_CONNECTION=database

# Metrics configuration
FINANCER_DISABLED_METRICS=

CACHE_STORE=database
CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS Configuration (for Cognito and Production S3)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=

# MinIO Configuration (Local S3 Emulation)
# These are separate from AWS credentials to allow both Cognito and MinIO to work simultaneously
MINIO_ROOT_USER=minio
MINIO_ROOT_PASSWORD=minio123
MINIO_ACCESS_KEY_ID=minio
MINIO_SECRET_ACCESS_KEY=minio123
MINIO_BUCKET=upengage
MINIO_SECOND_BUCKET=localization-repository
MINIO_ENDPOINT=http://minio:9000
MINIO_URL=http://localhost:9100/upengage
S3_USE_PATH_STYLE_ENDPOINT=true

VITE_APP_NAME="${APP_NAME}"

AWS_COGNITO_REGION=eu-west-3
AWS_COGNITO_USER_POOL_ID=eu-west-3_xxxxxxx
AWS_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxx
AWS_COGNITO_CLIENT_SECRET=xxxxxxxxxxxxxxxx

OPENAI_API_KEY=
OPENAI_ORGANIZATION=

# Maximum tokens for LLM article generation (default: 3000)
# 1 token â‰ˆ 0.75 words | Recommended: 2000 (short), 3000 (standard), 4500 (detailed)
OPENAI_MAX_TOKENS=4500

# OneSignal Configuration
ONESIGNAL_APP_ID=your-app-id-here
ONESIGNAL_REST_API_KEY=your-rest-api-key-here
ONESIGNAL_USER_AUTH_KEY=your-user-auth-key-here
ONESIGNAL_WEBHOOK_SECRET=your-webhook-secret-here

# Apideck Configuration
APIDECK_API_KEY=your_apideck_api_key
APIDECK_APP_ID=your_apideck_app_id
APIDECK_CONSUMER_ID=your_consumer_id
APIDECK_BASE_URL=https://unify.apideck.com
# Available service IDs: bamboohr, personio, workday, hibob, namely, sage-hr, adp-workforce-now
APIDECK_SERVICE_ID=bamboohr

# Vault Configuration
APIDECK_VAULT_SESSION_DURATION=3600
# Default SIRH service for Vault sessions (optional - if not set, user can choose from all available services)
# Available: bamboohr, personio, workday, hibob, namely, sage-hr, adp-workforce-now
APIDECK_DEFAULT_SERVICE_ID=
VAULT_RATE_LIMIT_MAX_ATTEMPTS=10
VAULT_RATE_LIMIT_DECAY_MINUTES=60

# Stripe Configuration
STRIPE_KEY=pk_test_xxxxxxxxxxxxxxxxxxxxxxxxxx
STRIPE_SECRET=sk_test_xxxxxxxxxxxxxxxxxxxxxxxxxx
STRIPE_WEBHOOK_SECRET=whsec_xxxxxxxxxxxxxxxxxxxxxxxxxx


WELLWO_API_URL=https://my.wellwo.net/api/v1
WELLWO_AUTH_TOKEN=yMXXXXXX
WELLWO_TIMEOUT=30

# Slack Configuration
SLACK_BOT_USER_OAUTH_TOKEN=xoxb-your-bot-token
SLACK_BOT_USER_DEFAULT_CHANNEL=#general
SLACK_BOT_USERNAME=MyBot
SLACK_BOT_ICON_EMOJI=:robot_face:
SLACK_TIMEOUT=30
SLACK_RETRY_TIMES=3
SLACK_RETRY_DELAY=100

# Slack Channels
SLACK_CHANNEL_ALERTS=#alerts
SLACK_CHANNEL_ERRORS=#errors
SLACK_CHANNEL_DEPLOYMENTS=#deployments
SLACK_CHANNEL_MONITORING=#monitoring

# Slack Webhooks (optional)
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# User Login Monitoring
# Enable/disable user login monitoring with Slack notifications
MONITORING_USER_LOGIN_ENABLED=false
# Comma-separated list of Cognito User IDs to monitor
MONITORING_USER_LOGIN_USER_IDS=0199a425-d226-709d-a3b3-feb52e7f7393
# Slack channel for login notifications
MONITORING_USER_LOGIN_SLACK_CHANNEL=up-engage-tech
# Throttle period in minutes (prevent notification spam)
MONITORING_USER_LOGIN_THROTTLE_MINUTES=5


WELLWO_RETRY_TIMES=3
WELLWO_RETRY_DELAY=100
WELLWO_CACHE_TTL=300
WELLWO_FILTER_BY_LANGUAGE=false

# Translation Protection Configuration
# Controls whether translations auto-sync in local environment
TRANSLATIONS_AUTO_SYNC_LOCAL=false

# Enables periodic reconciliation of translation files
TRANSLATIONS_RECONCILE_ENABLED=true

# Cron expression for reconciliation schedule (hourly by default)
TRANSLATIONS_RECONCILE_CRON="0 * * * *"

# Requires manifest validation in staging/production
TRANSLATIONS_MANIFEST_REQUIRED=true

# Demo accounts configuration
DEMO_ALLOWED=false
