{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "engage/main-api",
    "version": "0.1.0-dev",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "ext-pdo": "*",
        "aws/aws-sdk-php": "^3.339",
        "bensampo/laravel-enum": "^6.11",
        "berkayk/onesignal-laravel": "^2.4",
        "coconutcraig/laravel-postmark": "^3.3",
        "dedoc/scramble": "^0.12.35",
        "doctrine/dbal": "^4.3",
        "dompdf/dompdf": "^2.0",
        "fakerphp/faker": "^v1.24.1",
        "firebase/php-jwt": "^6.11",
        "laravel/framework": "^v12.1.1",
        "laravel/horizon": "^5.31",
        "laravel/pulse": "^1.4",
        "laravel/reverb": "^1.0",
        "laravel/telescope": "^5.6",
        "laravel/tinker": "^2.9",
        "league/commonmark": "^2.4",
        "league/flysystem-aws-s3-v3": "^3.29",
        "league/html-to-markdown": "^5.1",
        "livewire/livewire": "^3.6",
        "maatwebsite/excel": "^3.1",
        "maestroerror/php-heic-to-jpg": "^1.0",
        "mis3085/tiktoken-for-laravel": "^0.1.4",
        "nadar/prosemirror-json-parser": "^1.2",
        "opcodesio/log-viewer": "^3.15",
        "openai-php/client": "^0.13.0",
        "owen-it/laravel-auditing": "*",
        "phlak/semver": "^6.1",
        "phpoffice/phpspreadsheet": "^1.30",
        "predis/predis": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "sentry/sentry-laravel": "^4.13",
        "spatie/db-dumper": "^3.8",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-event-sourcing": "^7.11",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-permission": "^6.15",
        "spatie/laravel-ray": "^1.39",
        "spatie/laravel-settings": "^3.5",
        "spatie/laravel-translatable": "^6.11",
        "staudenmeir/eloquent-has-many-deep": "^1.21",
        "stripe/stripe-php": "^17.3",
        "timokoerber/laravel-one-time-operations": "^1.4"
    },
    "require-dev": {
        "ext-redis": "*",
        "barryvdh/laravel-ide-helper": "^3.5",
        "brianium/paratest": "^7.0",
        "ergebnis/phpstan-rules": "^2.6",
        "fakerphp/faker": "^1.23",
        "kitloong/laravel-migrations-generator": "^7.1",
        "larastan/larastan": "^v3.1.0",
        "laravel-lang/lang": "^15.26",
        "laravel/boost": "^1.6",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.20",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.1",
        "orangehill/iseed": "^3.1",
        "phpunit/phpunit": "^11.0.1",
        "rector/rector": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "App\\Integration\\": "app/integration/"
        },
        "files": [
            "app/Helpers/auth.php",
            "app/Helpers/helpers.php",
            "app/Helpers/cache_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ],
        "lint": [
            "./vendor/bin/pint"
        ],
        "rector": [
            "./vendor/bin/rector process"
        ],
        "stan": [
            "./vendor/bin/phpstan analyse --memory-limit=2G"
        ],
        "quality": [
            "@lint",
            "@rector",
            "@stan"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "App/Integrations/InternalCommunication/Tests"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
